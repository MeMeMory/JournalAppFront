{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/layout/(.*)$",
    "^@/ui/(.*)$",
    "^@/providers/(.*)$",
    "^@/shared/(.*)$",
    "^@/assets/(.*)$",
    "^@/config/(.*)$",
    "^@/store/(.*)$",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^@/api/(.*)$",
    "^../(.*)$",
    "^./(.*)$"
  ],
  "overrides": [
    {
      "files": "*.{js,ts,jsx,tsx}",
      "options": {
        "prettier": false
      }
    }
  ],
  "importOrderSeparation": true,
  "importOrderSortSeparation": true
}
